{
  "name": "sapphire-vscode-syntax-highlighter",
  "displayName": "Sapphire",
  "description": "A simple extension for Sapphire syntax highlighting.",
  "version": "0.0.1",
  "publisher": "jabraham17",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sapphire",
        "aliases": [
          "sapphire"
        ],
        "extensions": [
          ".spp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sapphire",
        "scopeName": "source.spp",
        "path": "./syntaxes/sapphire.tmLanguage.json"
      }
    ]
  }
}
